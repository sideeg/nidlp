<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">
    <t t-name="DashboardView">
        <div class="contract_dashboard">
            <div class="container-fluid odashboard">
                <div class="sales-dashboard">
                  <h4 class="text-primary text-center py-5">Contract Dashboard</h4>
                    <div class="container">
                    <div class="contract-cards pb-5">
                      <!-- Start: Contract Cards -->
                        <div class="row">
                        <div class="col-lg-4 col-sm-6 col-12">
                          <div class="appointment_cards position-relative overflow-hidden appointment_card1 rounded p-sm-4 p-3 more-new-contract">
                            <i class="fa fa-plus-circle position-absolute opacity_icon"></i>
                              <div class="row align-items-center">
                              <div class="col-3 pr-0">
                                <h1 class="card_number_text m-0 text-white"><b class="new-contract"></b></h1>
                              </div>
                                  <div class="col-9">
                                <h4 class="mb-1 text-white card_up_title">Contract</h4>
                                      <h2 class="m-0 text-white card_main_title">New</h2>
                              </div>
                            </div>
                          </div>
                        </div>
                            <div class="col-lg-4 col-sm-6 col-12">
                          <div class="appointment_cards position-relative overflow-hidden appointment_card2 rounded p-sm-4 p-3 more-running-contract">
                            <i class="fa fa-th-list position-absolute opacity_icon"></i>
                              <div class="row align-items-center">
                              <div class="col-3 pr-0">
                                <h1 class="card_number_text m-0 text-white"><b class="running-contract"></b></h1>
                              </div>
                                  <div class="col-9">
                                <h4 class="mb-1 text-white card_up_title">Contract</h4>
                                      <h2 class="m-0 text-white card_main_title">Running</h2>
                              </div>
                            </div>
                          </div>
                        </div>
                            <div class="col-lg-4 col-sm-6 col-12">
                          <div class="appointment_cards position-relative overflow-hidden appointment_card3 rounded p-sm-4 p-3 more-confirm-contract">
                            <i class="fa fa-clock-o position-absolute opacity_icon"></i>
                              <div class="row align-items-center">
                              <div class="col-3 pr-0">
                                <h1 class="card_number_text m-0 text-white"><b class="confirm-contract"></b></h1>
                              </div>
                                  <div class="col-9">
                                <h4 class="mb-1 text-white card_up_title">Contract</h4>
                                      <h2 class="m-0 text-white card_main_title">Confirm</h2>
                              </div>
                            </div>
                          </div>
                        </div>
                            <div class="col-lg-4 col-sm-6 col-12">
                          <div class="appointment_cards position-relative overflow-hidden appointment_card4 rounded p-sm-4 p-3 more-stop-contract">
                            <i class="fa fa-times position-absolute opacity_icon"></i>
                              <div class="row align-items-center">
                              <div class="col-3 pr-0">
                                <h1 class="card_number_text m-0 text-white"><b class="stop-contract"></b></h1>
                              </div>
                                  <div class="col-9">
                                <h4 class="mb-1 text-white card_up_title">Contract</h4>
                                      <h2 class="m-0 text-white card_main_title">Stop</h2>
                              </div>
                            </div>
                          </div>
                        </div>
                            <div class="col-lg-4 col-sm-6 col-12">
                          <div class="appointment_cards position-relative overflow-hidden appointment_card6 rounded p-sm-4 p-3 more-total-contract">
                            <i class="fa fa-pencil-square position-absolute opacity_icon"></i>
                              <div class="row align-items-center">
                              <div class="col-3 pr-0">
                                <h1 class="card_number_text m-0 text-white"><b class="total-contract"></b></h1>
                              </div>
                                  <div class="col-9">
                                <h4 class="mb-1 text-white card_up_title">Total</h4>
                                      <h2 class="m-0 text-white card_main_title">Contract</h2>
                              </div>
                            </div>
                          </div>
                        </div>
                            <div class="col-lg-4 col-sm-6 col-12">
                          <div class="appointment_cards position-relative overflow-hidden appointment_card7 rounded p-sm-4 p-3 total_delivered">
                            <i class="fa fa-dropbox position-absolute opacity_icon"></i>
                              <div class="row align-items-center">
                              <div class="col-3 pr-0">
                                <h1 class="card_number_text m-0 text-white"><b class="total-delivered"></b></h1>
                              </div>
                                  <div class="col-9">
                                <h4 class="mb-1 text-white card_up_title">Total</h4>
                                      <h2 class="m-0 text-white card_main_title">Contract Delivered Order</h2>
                              </div>
                            </div>
                          </div>
                        </div>
                            <div class="col-lg-4 col-sm-6 col-12">
                          <div class="appointment_cards position-relative overflow-hidden appointment_card9 rounded p-sm-4 p-3 total_invoice">
                            <i class="fa fa-file position-absolute opacity_icon"></i>
                              <div class="row align-items-center">
                              <div class="col-3 pr-0">
                                <h1 class="card_number_text m-0 text-white"><b class="total-invoice"></b></h1>
                              </div>
                                  <div class="col-9">
                                <h4 class="mb-1 text-white card_up_title">Total</h4>
                                      <h2 class="m-0 text-white card_main_title">Invoice</h2>
                              </div>
                            </div>
                          </div>
                        </div>
                      </div>
                        <!-- End: Appointment Cards -->
                    </div>
                  </div>
                </div>
              </div>
          </div>
      </t>
</templates>
